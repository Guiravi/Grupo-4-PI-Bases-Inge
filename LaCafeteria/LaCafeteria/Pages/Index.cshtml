@page
@using LaCafeteria.Utilidades;
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<br/>

<div class="container"> 
    <div class="row">
        <div class="col-md-12">
			@if(Notificaciones.Existe(Model, "sesionIniciada"))
			{
				@Html.Raw(Notificaciones.Get(Model, "sesionIniciada"));
			}
            <h1>Bienvenidos a la comunidad de práctica La Cafetería</h1>
        </div>
    </div>
    <div class="row">
        <div class="col-md-6">
            <h3>Desarrolladores:</h3>
            <ul>
                <li>Juan Jose Herrera</li>
                <li>Marco Mora</li>
                <li>Guillermo Ramírez</li>
                <li>Silvio Salazar</li>
                <li>Sheng Qi Wang</li>
            </ul>
        </div>
        <div class="col-md-6">
            <img src="~/images/logoLC.svg" class="img-responsive" style="width: 50%; height: 50%;"/>
        </div>
    </div>
</div>



